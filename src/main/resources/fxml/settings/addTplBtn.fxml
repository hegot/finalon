<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" styleClass="addTemplate" type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/8.0.121" maxWidth="900">
    <Label styleClass="add-template-text" text="$labelText" textFill="#6a6c6f"/>

    <Button styleClass="add-template-btn" text="Add Template" onAction="#addTemplateAction"/>
</fx:root>

